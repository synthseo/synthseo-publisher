{
  "name": "synthseo-publisher",
  "version": "2.0.2",
  "description": "WordPress plugin for SynthSEO content publishing with React and TailwindCSS",
  "author": "SynthSEO",
  "license": "GPL-2.0-or-later",
  "scripts": {
    "start": "NODE_ENV=development webpack --watch --config webpack.config.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "NODE_ENV=development webpack --config webpack.config.js",
    "analyze": "NODE_ENV=production webpack --config webpack.config.js --analyze",
    "lint:js": "wp-scripts lint-js src",
    "lint:css": "wp-scripts lint-style src/**/*.css",
    "format": "wp-scripts format",
    "test": "wp-scripts test-unit-js",
    "test:watch": "wp-scripts test-unit-js --watch",
    "clean": "rm -rf build release *.zip",
    "release": "./build-release.sh",
    "release:test": "npm run clean && npm run build && ./build-release.sh",
    "prebuild": "rm -rf build",
    "dev": "npm run start",
    "dev:build": "npm run build:dev"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@wordpress/scripts": "^30.5.1",
    "babel-loader": "^10.0.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "express": "^4.21.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.14.0",
    "@wordpress/components": "^29.0.0",
    "@wordpress/compose": "^7.14.0",
    "@wordpress/data": "^10.14.0",
    "@wordpress/element": "^6.14.0",
    "@wordpress/hooks": "^4.14.0",
    "@wordpress/i18n": "^5.14.0",
    "@wordpress/notices": "^5.14.0",
    "classnames": "^2.5.1"
  }
}
